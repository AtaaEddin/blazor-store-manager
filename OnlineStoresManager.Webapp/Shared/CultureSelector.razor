@inherits OSMComponent

<MudSelect FullWidth="true" T="string" Label="@Localizer["Language"]" @bind-Value=@CurrentCultureName Variant="Variant.Filled" AnchorOrigin="Origin.BottomCenter">
    @foreach (KeyValuePair<string, string> keyValuePair in SupportedCultures)
    {
        <MudSelectItem Value="@(keyValuePair.Value)" />
    }
</MudSelect>