@inherits OSMComponent

<MudNavMenu>
    <AuthorizeView Roles=@Roles.Administrator>
        <Authorized>
            <MudNavLink Href="admin/dashboard" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home">@Localizer["Dashboard"]</MudNavLink>
            <MudNavLink Href="books" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">@Localizer["Books"]</MudNavLink>
            <MudNavLink Href="shirts" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">@Localizer["Shirts"]</MudNavLink>
        </Authorized>
    </AuthorizeView>
</MudNavMenu>