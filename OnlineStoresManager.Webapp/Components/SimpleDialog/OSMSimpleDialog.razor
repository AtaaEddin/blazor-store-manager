@inherits OSMComponent

<MudDialog>
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick=Cancel>@Localizer["Cancel"]</MudButton>
        <MudButton Color="@Color" Variant="Variant.Filled" OnClick=Submit>@ButtonText</MudButton>
    </DialogActions>
</MudDialog>

